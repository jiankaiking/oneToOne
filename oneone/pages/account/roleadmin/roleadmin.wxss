@import "../../../static/css/flex";

page {
  background: #f6f6f6;
}

.pages-hand {
  height: 70rpx;
  background: #fff;
}

.hand-select image {
  width: 11rpx;
  padding-left: 12rpx;
  height: 11rpx;
}

.hand-add {
  font-size: 24rpx;
  color: #3b99fa;
}

.hand-select {
  color: #666;
  font-size: 24rpx;
}

.hand-add image {
  width: 30rpx;
  padding-right: 12rpx;
  height: 30rpx;
}

.item-content {
  margin-top: 16rpx;
  background: #fff;
  font-size: 28rpx;
  color: #333;
}

.title {
  padding: 25rpx 0;
  border-bottom: 1rpx solid #f6f6f6;
}

.item:last-child .title {
  border-bottom: none;
}
